'.text.html':
  'cards:align':
    prefix: 'cards:align'
    body: '''
      <div class="card card-block text-{1:center|right}">
      			<h4 class="card-title">${2:Special title treatment}</h4>
      			<p class="card-text">${3:With supporting text below as a natural lead-in to additional content.}</p>
      			<a href="${4:#}" class="btn btn-primary">${5:Go somewhere}</a>
      		</div>$0
    '''