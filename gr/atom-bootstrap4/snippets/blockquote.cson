'.text.html':
  'typography:blockquotes':
    prefix: 'typography:blockquotes'
    body: '''
      <blockquote class="blockquote">
      	<p>${1:Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere erat a ante.}</p>
